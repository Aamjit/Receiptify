{
  "expo": {
    "name": "Receiptify",
    "slug": "receiptify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/receiptify-icon.png",
    "scheme": "receiptify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.receiptify.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/receiptify-icon.png",
        "backgroundColor": "#fbfbfb"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.receiptify.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "@/assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-router",
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "imageHeight": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb56e57c-9245-4ada-abe9-f673e86dc57a"
      }
    },
    "owner": "aamjit"
  }
}